<div>
    <h1 class="card z-depth-3 center-align">Read the latest from the Medical News Today</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click scrape to get the latest articles</h5>
    </div>
</div>
<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this.id}}" class="wavws-effect waves light btn" id="btnSignUp">Read Article</a>
        </div>
        {{/each}}
    </div>
</div>